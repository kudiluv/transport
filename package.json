{
    "name": "transport",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm --d=src/ormconfig.ts",
        "db:drop": "npm run typeorm schema:drop",
        "db:migration-create": "npm run typeorm migration:generate  ./src/migrations/$npm_config_name",
        "db:migration-revert": "npm run typeorm migration:revert",
        "db:migrate": "npm run typeorm migration:run"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.12.5",
        "@grpc/proto-loader": "^0.7.13",
        "@nestjs-cls/transactional": "^2.4.4",
        "@nestjs-cls/transactional-adapter-typeorm": "^1.2.6",
        "@nestjs/axios": "^4.0.0",
        "@nestjs/bullmq": "^11.0.1",
        "@nestjs/common": "^11.0.3",
        "@nestjs/config": "^4.0.0",
        "@nestjs/core": "^11.0.3",
        "@nestjs/microservices": "^11.0.3",
        "@nestjs/platform-express": "^11.0.3",
        "@nestjs/platform-fastify": "^11.0.3",
        "@nestjs/swagger": "^11.0.2",
        "@nestjs/typeorm": "^11.0.0",
        "@turf/turf": "^7.2.0",
        "axios": "^1.7.9",
        "bullmq": "^5.34.10",
        "cbor2": "^1.9.0",
        "google-maps": "^4.3.3",
        "google-polyline": "^1.0.3",
        "lodash": "^4.17.21",
        "msgpackr": "^1.11.2",
        "nestjs-cls": "^4.5.0",
        "nestjs-zod": "^4.2.0",
        "pbf": "^4.0.1",
        "pg": "^8.13.1",
        "protocol-buffers-schema": "^3.6.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "transliteration": "^2.3.5",
        "ts-protoc-gen": "^0.15.0",
        "typeorm": "^0.3.20",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^11.0.1",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.3",
        "@types/express": "^5.0.0",
        "@types/google-polyline": "^1.0.2",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.14",
        "@types/node": "^22.10.7",
        "@types/pbf": "^3.0.5",
        "@types/protocol-buffers-schema": "^3.4.3",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.7.0",
        "prettier": "^3.4.2",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
